{
  "name": "hydro-ui",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.18.0",
    "date-fns": "^2.0.0-alpha.25",
    "evergreen-ui": "^4.15.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-quill": "^1.3.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "styled-components": "^4.2.0",
    "vertx3-eventbus-client": "^3.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./src/**/*.js\"",
    "format:ci": "prettier --list-different \"./src/**/*.js\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "1.16.3"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "always",
    "trailingComma": "all",
    "singleQuote": true
  }
}
